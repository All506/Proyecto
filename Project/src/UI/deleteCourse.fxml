      <ComboBox fx:id="cmbAvailableCourses" layoutX="307.0" layoutY="115.0" onMouseClicked="#AvailableCoursesOnMouseClicked" prefHeight="25.0" prefWidth="178.0" promptText="Cursos disponibles" visible="false" />
